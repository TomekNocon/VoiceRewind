{
  "manifest_version": 3,
  "name": "VoiceRewind",
  "version": "0.1.0",
  "description": "Voice controls and transcript jump for YouTube.",
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
} 